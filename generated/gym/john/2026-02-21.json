{
  "root": "main-stack",
  "elements": {
    "main-stack": {
      "type": "Stack",
      "props": { "gap": "lg" },
      "children": ["header-card", "warmup-card", "main-lifts-card", "accessories-card", "cooldown-card", "session-progress-card"]
    },
    "header-card": {
      "type": "Card",
      "props": {},
      "children": ["header-stack"]
    },
    "header-stack": {
      "type": "Stack",
      "props": { "gap": "sm" },
      "children": ["header-title", "header-subtitle", "header-badges"]
    },
    "header-title": {
      "type": "Heading",
      "props": { "level": 2, "text": "Pull Day — Deadlift Focus" }
    },
    "header-subtitle": {
      "type": "Text",
      "props": { "text": "Saturday, February 21 2026 — Estimated 55 min", "variant": "muted" }
    },
    "header-badges": {
      "type": "Stack",
      "props": { "direction": "horizontal", "gap": "sm" },
      "children": ["badge-back", "badge-biceps", "badge-rear-delts", "badge-hamstrings"]
    },
    "badge-back": {
      "type": "Badge",
      "props": { "text": "Back" }
    },
    "badge-biceps": {
      "type": "Badge",
      "props": { "text": "Biceps" }
    },
    "badge-rear-delts": {
      "type": "Badge",
      "props": { "text": "Rear Delts" }
    },
    "badge-hamstrings": {
      "type": "Badge",
      "props": { "text": "Hamstrings" }
    },
    "warmup-card": {
      "type": "Card",
      "props": {},
      "children": ["warmup-stack"]
    },
    "warmup-stack": {
      "type": "Stack",
      "props": { "gap": "md" },
      "children": ["warmup-title", "warmup-alert", "warmup-table", "warmup-done"]
    },
    "warmup-title": {
      "type": "Heading",
      "props": { "level": 3, "text": "Warmup" }
    },
    "warmup-alert": {
      "type": "Alert",
      "props": {
        "title": "Deadlift prep",
        "description": "Prime your hinge pattern with light RDLs. Brace hard on every rep — belt optional until working sets above 100kg.",
        "variant": "warning"
      }
    },
    "warmup-table": {
      "type": "Table",
      "props": {
        "columns": [
          { "key": "exercise", "header": "Exercise" },
          { "key": "duration", "header": "Sets / Duration" }
        ],
        "rows": [
          { "exercise": "Cat-cow + hip circles", "duration": "1 min" },
          { "exercise": "Band pull-aparts", "duration": "2 x 15" },
          { "exercise": "Light RDL (empty bar)", "duration": "2 x 8" },
          { "exercise": "Dead hang", "duration": "2 x 20s" }
        ]
      }
    },
    "warmup-done": {
      "type": "Checkbox",
      "props": {
        "label": "Warmup completed",
        "checked": { "$bindState": "/completed/warmup" }
      }
    },
    "main-lifts-card": {
      "type": "Card",
      "props": {},
      "children": ["main-lifts-stack"]
    },
    "main-lifts-stack": {
      "type": "Stack",
      "props": { "gap": "md" },
      "children": ["main-lifts-title", "deadlift-alert", "main-lifts-table", "main-lifts-done"]
    },
    "main-lifts-title": {
      "type": "Heading",
      "props": { "level": 3, "text": "Main Lifts" }
    },
    "deadlift-alert": {
      "type": "Alert",
      "props": {
        "title": "Deadlift cue",
        "description": "Push the floor away. Squeeze lats before the pull — imagine bending the bar around your shins. Current PR: 140kg — today's top set is ~85% for controlled triples.",
        "variant": "info"
      }
    },
    "main-lifts-table": {
      "type": "Table",
      "props": {
        "columns": [
          { "key": "exercise", "header": "Exercise" },
          { "key": "sets", "header": "Sets" },
          { "key": "reps", "header": "Reps" },
          { "key": "rest", "header": "Rest" },
          { "key": "rpe", "header": "RPE" }
        ],
        "rows": [
          { "exercise": "Conventional Deadlift", "sets": "4", "reps": "3", "rest": "3 min", "rpe": "8" },
          { "exercise": "Barbell Row (Pendlay)", "sets": "4", "reps": "6-8", "rest": "2 min", "rpe": "7-8" }
        ]
      }
    },
    "main-lifts-done": {
      "type": "Checkbox",
      "props": {
        "label": "Main lifts completed",
        "checked": { "$bindState": "/completed/mainLifts" }
      }
    },
    "accessories-card": {
      "type": "Card",
      "props": {},
      "children": ["accessories-stack"]
    },
    "accessories-stack": {
      "type": "Stack",
      "props": { "gap": "md" },
      "children": ["accessories-title", "accessories-table", "accessories-accordion", "accessories-done"]
    },
    "accessories-title": {
      "type": "Heading",
      "props": { "level": 3, "text": "Accessories (Supersets)" }
    },
    "accessories-table": {
      "type": "Table",
      "props": {
        "columns": [
          { "key": "superset", "header": "Superset" },
          { "key": "exercise", "header": "Exercise" },
          { "key": "sets", "header": "Sets" },
          { "key": "reps", "header": "Reps" }
        ],
        "rows": [
          { "superset": "A", "exercise": "Lat Pulldown", "sets": "3", "reps": "10-12" },
          { "superset": "A", "exercise": "Barbell Curl", "sets": "3", "reps": "10-12" },
          { "superset": "B", "exercise": "Face Pull", "sets": "3", "reps": "15-20" },
          { "superset": "B", "exercise": "Hammer Curl", "sets": "3", "reps": "12" },
          { "superset": "C", "exercise": "Seated Cable Row", "sets": "3", "reps": "12-15" },
          { "superset": "C", "exercise": "Reverse Fly (machine)", "sets": "3", "reps": "15" }
        ]
      }
    },
    "accessories-accordion": {
      "type": "Accordion",
      "props": {
        "items": [
          {
            "title": "Alternative swaps",
            "content": "Lat Pulldown → Pull-ups (weighted if possible). Barbell Curl → Incline DB Curl. Seated Cable Row → Chest-Supported Row. Reverse Fly → Band pull-aparts 3x25."
          }
        ]
      }
    },
    "accessories-done": {
      "type": "Checkbox",
      "props": {
        "label": "Accessories completed",
        "checked": { "$bindState": "/completed/accessories" }
      }
    },
    "cooldown-card": {
      "type": "Card",
      "props": {},
      "children": ["cooldown-stack"]
    },
    "cooldown-stack": {
      "type": "Stack",
      "props": { "gap": "md" },
      "children": ["cooldown-title", "cooldown-table", "cooldown-done"]
    },
    "cooldown-title": {
      "type": "Heading",
      "props": { "level": 3, "text": "Cooldown" }
    },
    "cooldown-table": {
      "type": "Table",
      "props": {
        "columns": [
          { "key": "exercise", "header": "Exercise" },
          { "key": "duration", "header": "Duration" }
        ],
        "rows": [
          { "exercise": "Lat stretch (hang from bar)", "duration": "30s each side" },
          { "exercise": "Seated hamstring stretch", "duration": "45s each leg" },
          { "exercise": "Foam roll lower back & lats", "duration": "2 min" },
          { "exercise": "Child's pose", "duration": "1 min" }
        ]
      }
    },
    "cooldown-done": {
      "type": "Checkbox",
      "props": {
        "label": "Cooldown completed",
        "checked": { "$bindState": "/completed/cooldown" }
      }
    },
    "session-progress-card": {
      "type": "Card",
      "props": {},
      "children": ["session-progress-stack"]
    },
    "session-progress-stack": {
      "type": "Stack",
      "props": { "gap": "md" },
      "children": ["session-progress-title", "session-stats", "session-progress-bar"]
    },
    "session-progress-title": {
      "type": "Heading",
      "props": { "level": 3, "text": "Session Summary" }
    },
    "session-stats": {
      "type": "Table",
      "props": {
        "columns": [
          { "key": "metric", "header": "Metric" },
          { "key": "value", "header": "Value" }
        ],
        "rows": [
          { "metric": "Total working sets", "value": "26" },
          { "metric": "Estimated duration", "value": "~55 min" },
          { "metric": "Primary focus", "value": "Deadlift strength" },
          { "metric": "Split day", "value": "Pull" }
        ]
      }
    },
    "session-progress-bar": {
      "type": "Progress",
      "props": { "value": 0 }
    }
  },
  "state": {
    "/completed/warmup": false,
    "/completed/mainLifts": false,
    "/completed/accessories": false,
    "/completed/cooldown": false
  }
}
