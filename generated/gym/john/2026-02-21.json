{
  "root": "page",
  "elements": {
    "page": {
      "type": "Stack",
      "props": { "gap": "lg" },
      "children": ["header-card", "muscle-badges", "warmup-card", "main-lifts-card", "accessories-card", "cooldown-card", "swaps-card"]
    },
    "header-card": {
      "type": "Card",
      "props": {},
      "children": ["header-stack"]
    },
    "header-stack": {
      "type": "Stack",
      "props": { "gap": "sm" },
      "children": ["header-title", "header-subtitle", "header-text"]
    },
    "header-title": {
      "type": "Heading",
      "props": { "level": "h2", "text": "Saturday Pull — Deadlift Focus" }
    },
    "header-subtitle": {
      "type": "Text",
      "props": { "text": "Saturday, February 21 2026 — Estimated 55 min", "variant": "muted" }
    },
    "header-text": {
      "type": "Text",
      "props": { "text": "Heavy deadlift day with back and bicep accessories. Keep rest periods strict on accessories to stay within time.", "variant": "body" }
    },
    "muscle-badges": {
      "type": "Stack",
      "props": { "direction": "horizontal", "gap": "sm", "align": "center" },
      "children": ["badge-back", "badge-hamstrings", "badge-biceps", "badge-rear-delts", "badge-grip"]
    },
    "badge-back": { "type": "Badge", "props": { "text": "Back", "variant": "default" } },
    "badge-hamstrings": { "type": "Badge", "props": { "text": "Hamstrings", "variant": "default" } },
    "badge-biceps": { "type": "Badge", "props": { "text": "Biceps", "variant": "secondary" } },
    "badge-rear-delts": { "type": "Badge", "props": { "text": "Rear Delts", "variant": "secondary" } },
    "badge-grip": { "type": "Badge", "props": { "text": "Grip", "variant": "outline" } },

    "warmup-card": {
      "type": "Card",
      "props": {},
      "children": ["warmup-heading", "warmup-table", "warmup-alert", "warmup-check"]
    },
    "warmup-heading": { "type": "Heading", "props": { "text": "Warmup", "level": "h3" } },
    "warmup-table": {
      "type": "Table",
      "props": {
        "columns": ["Exercise", "Duration / Reps", "Notes"],
        "rows": [
          ["Rowing Machine", "3 min", "Easy pace, loosen up"],
          ["Cat-Cow Stretch", "10 reps", "Slow and controlled"],
          ["Band Pull-Aparts", "2 × 15", "Light band, squeeze rear delts"],
          ["Hip Hinge with Dowel", "2 × 10", "Groove the hinge pattern"],
          ["Deadlift — Empty Bar", "2 × 8", "Focus on brace and lockout"]
        ]
      }
    },
    "warmup-alert": {
      "type": "Alert",
      "props": { "title": "Brace before every rep", "message": "Big breath into your belt, push obliques out. Reset between reps on the working sets.", "type": "info" }
    },
    "warmup-check": {
      "type": "Checkbox",
      "props": { "label": "Warmup complete", "name": "warmup-done", "checked": { "$bindState": "/completed/warmup" } }
    },

    "main-lifts-card": {
      "type": "Card",
      "props": {},
      "children": ["main-heading", "main-table", "main-alert", "main-check"]
    },
    "main-heading": { "type": "Heading", "props": { "text": "Main Lifts", "level": "h3" } },
    "main-table": {
      "type": "Table",
      "props": {
        "columns": ["Exercise", "Sets", "Reps", "Rest", "RPE", "Notes"],
        "rows": [
          ["Conventional Deadlift", "4", "3", "3 min", "8–9", "Work up: 100 → 110 → 120 → 130 kg"],
          ["Deadlift Back-Off", "1", "5", "—", "7", "110 kg — controlled tempo"],
          ["Barbell Row (Pendlay)", "4", "6", "2 min", "8", "Pull to lower chest, pause at top"]
        ]
      }
    },
    "main-alert": {
      "type": "Alert",
      "props": { "title": "Deadlift cue", "message": "Push the floor away with your legs, don't yank with your back. Squeeze glutes hard at lockout. Building toward that 140 kg+ PR — keep form tight.", "type": "warning" }
    },
    "main-check": {
      "type": "Checkbox",
      "props": { "label": "Main lifts complete", "name": "main-done", "checked": { "$bindState": "/completed/mainLifts" } }
    },

    "accessories-card": {
      "type": "Card",
      "props": {},
      "children": ["acc-heading", "acc-note", "acc-table", "acc-check"]
    },
    "acc-heading": { "type": "Heading", "props": { "text": "Accessories", "level": "h3" } },
    "acc-note": { "type": "Text", "props": { "text": "Superset pairs A1/A2 and B1/B2 — rest 60s between supersets.", "variant": "muted" } },
    "acc-table": {
      "type": "Table",
      "props": {
        "columns": ["Exercise", "Sets", "Reps", "Rest", "RPE"],
        "rows": [
          ["A1 — Chest-Supported Row", "3", "10", "—", "8"],
          ["A2 — Dumbbell Hammer Curl", "3", "12", "60s", "7–8"],
          ["B1 — Face Pull", "3", "15", "—", "7"],
          ["B2 — Plate Pinch Hold", "3", "30s", "60s", "8"]
        ]
      }
    },
    "acc-check": {
      "type": "Checkbox",
      "props": { "label": "Accessories complete", "name": "acc-done", "checked": { "$bindState": "/completed/accessories" } }
    },

    "cooldown-card": {
      "type": "Card",
      "props": {},
      "children": ["cool-heading", "cool-table", "cool-check"]
    },
    "cool-heading": { "type": "Heading", "props": { "text": "Cooldown", "level": "h3" } },
    "cool-table": {
      "type": "Table",
      "props": {
        "columns": ["Exercise", "Duration", "Notes"],
        "rows": [
          ["Hanging from Bar", "2 × 30s", "Decompress spine after deads"],
          ["Seated Hamstring Stretch", "60s each side", "Breathe into the stretch"],
          ["Child's Pose", "60s", "Let lower back release"]
        ]
      }
    },
    "cool-check": {
      "type": "Checkbox",
      "props": { "label": "Cooldown complete", "name": "cool-done", "checked": { "$bindState": "/completed/cooldown" } }
    },

    "swaps-card": {
      "type": "Card",
      "props": {},
      "children": ["swaps-heading", "swaps-collapsible"]
    },
    "swaps-heading": { "type": "Heading", "props": { "text": "Exercise Swaps", "level": "h3" } },
    "swaps-collapsible": {
      "type": "Collapsible",
      "props": { "title": "View alternative exercises", "defaultOpen": false },
      "children": ["swaps-table"]
    },
    "swaps-table": {
      "type": "Table",
      "props": {
        "columns": ["Exercise", "Alternative"],
        "rows": [
          ["Conventional Deadlift", "Trap Bar Deadlift"],
          ["Barbell Row (Pendlay)", "Seal Row or T-Bar Row"],
          ["Chest-Supported Row", "Cable Row (neutral grip)"],
          ["Dumbbell Hammer Curl", "Cable Rope Curl"],
          ["Face Pull", "Reverse Pec Deck"],
          ["Plate Pinch Hold", "Farmer's Walk (30m)"]
        ]
      }
    }
  },
  "state": {
    "/completed/warmup": false,
    "/completed/mainLifts": false,
    "/completed/accessories": false,
    "/completed/cooldown": false
  }
}