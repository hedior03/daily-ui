{
  "root": "main-stack",
  "elements": {
    "main-stack": {
      "type": "Stack",
      "props": { "gap": "lg" },
      "children": ["header-card", "warmup-card", "main-lifts-card", "accessories-card", "cooldown-card", "session-progress-card"]
    },
    "header-card": {
      "type": "Card",
      "props": {},
      "children": ["header-stack"]
    },
    "header-stack": {
      "type": "Stack",
      "props": { "gap": "sm" },
      "children": ["header-title", "header-subtitle", "header-badges"]
    },
    "header-title": {
      "type": "Heading",
      "props": { "level": "h2", "text": "Pull Day — Deadlift Focus" }
    },
    "header-subtitle": {
      "type": "Text",
      "props": { "text": "Saturday, February 21 2026 — Estimated 55 min", "variant": "muted" }
    },
    "header-badges": {
      "type": "Stack",
      "props": { "direction": "horizontal", "gap": "sm" },
      "children": ["badge-back", "badge-biceps", "badge-rear-delts", "badge-hamstrings"]
    },
    "badge-back": {
      "type": "Badge",
      "props": { "text": "Back" }
    },
    "badge-biceps": {
      "type": "Badge",
      "props": { "text": "Biceps" }
    },
    "badge-rear-delts": {
      "type": "Badge",
      "props": { "text": "Rear Delts" }
    },
    "badge-hamstrings": {
      "type": "Badge",
      "props": { "text": "Hamstrings" }
    },
    "warmup-card": {
      "type": "Card",
      "props": {},
      "children": ["warmup-stack"]
    },
    "warmup-stack": {
      "type": "Stack",
      "props": { "gap": "md" },
      "children": ["warmup-title", "warmup-alert", "warmup-table", "warmup-done"]
    },
    "warmup-title": {
      "type": "Heading",
      "props": { "level": "h3", "text": "Warmup" }
    },
    "warmup-alert": {
      "type": "Alert",
      "props": {
        "title": "Deadlift prep",
        "message": "Prime your hinge pattern with light RDLs. Brace hard on every rep — belt optional until working sets above 100kg.",
        "type": "warning"
      }
    },
    "warmup-table": {
      "type": "Table",
      "props": {
        "columns": ["Exercise", "Sets / Duration"],
        "rows": [
          ["Cat-cow + hip circles", "1 min"],
          ["Band pull-aparts", "2 x 15"],
          ["Light RDL (empty bar)", "2 x 8"],
          ["Dead hang", "2 x 20s"]
        ]
      }
    },
    "warmup-done": {
      "type": "Checkbox",
      "props": {
        "label": "Warmup completed",
        "checked": { "$bindState": "/completed/warmup" }
      }
    },
    "main-lifts-card": {
      "type": "Card",
      "props": {},
      "children": ["main-lifts-stack"]
    },
    "main-lifts-stack": {
      "type": "Stack",
      "props": { "gap": "md" },
      "children": ["main-lifts-title", "deadlift-alert", "main-lifts-table", "main-lifts-done"]
    },
    "main-lifts-title": {
      "type": "Heading",
      "props": { "level": "h3", "text": "Main Lifts" }
    },
    "deadlift-alert": {
      "type": "Alert",
      "props": {
        "title": "Deadlift cue",
        "message": "Push the floor away. Squeeze lats before the pull — imagine bending the bar around your shins. Current PR: 140kg — today's top set is ~85% for controlled triples.",
        "type": "info"
      }
    },
    "main-lifts-table": {
      "type": "Table",
      "props": {
        "columns": ["Exercise", "Sets", "Reps", "Rest", "RPE"],
        "rows": [
          ["Conventional Deadlift", "4", "3", "3 min", "8"],
          ["Barbell Row (Pendlay)", "4", "6-8", "2 min", "7-8"]
        ]
      }
    },
    "main-lifts-done": {
      "type": "Checkbox",
      "props": {
        "label": "Main lifts completed",
        "checked": { "$bindState": "/completed/mainLifts" }
      }
    },
    "accessories-card": {
      "type": "Card",
      "props": {},
      "children": ["accessories-stack"]
    },
    "accessories-stack": {
      "type": "Stack",
      "props": { "gap": "md" },
      "children": ["accessories-title", "accessories-table", "accessories-swaps", "accessories-done"]
    },
    "accessories-title": {
      "type": "Heading",
      "props": { "level": "h3", "text": "Accessories (Supersets)" }
    },
    "accessories-table": {
      "type": "Table",
      "props": {
        "columns": ["Superset", "Exercise", "Sets", "Reps"],
        "rows": [
          ["A", "Lat Pulldown", "3", "10-12"],
          ["A", "Barbell Curl", "3", "10-12"],
          ["B", "Face Pull", "3", "15-20"],
          ["B", "Hammer Curl", "3", "12"],
          ["C", "Seated Cable Row", "3", "12-15"],
          ["C", "Reverse Fly (machine)", "3", "15"]
        ]
      }
    },
    "accessories-swaps": {
      "type": "Collapsible",
      "props": { "title": "Alternative swaps" },
      "children": ["accessories-swaps-table"]
    },
    "accessories-swaps-table": {
      "type": "Table",
      "props": {
        "columns": ["Exercise", "Alternative"],
        "rows": [
          ["Lat Pulldown", "Pull-ups (weighted if possible)"],
          ["Barbell Curl", "Incline DB Curl"],
          ["Seated Cable Row", "Chest-Supported Row"],
          ["Reverse Fly", "Band pull-aparts 3x25"]
        ]
      }
    },
    "accessories-done": {
      "type": "Checkbox",
      "props": {
        "label": "Accessories completed",
        "checked": { "$bindState": "/completed/accessories" }
      }
    },
    "cooldown-card": {
      "type": "Card",
      "props": {},
      "children": ["cooldown-stack"]
    },
    "cooldown-stack": {
      "type": "Stack",
      "props": { "gap": "md" },
      "children": ["cooldown-title", "cooldown-table", "cooldown-done"]
    },
    "cooldown-title": {
      "type": "Heading",
      "props": { "level": "h3", "text": "Cooldown" }
    },
    "cooldown-table": {
      "type": "Table",
      "props": {
        "columns": ["Exercise", "Duration"],
        "rows": [
          ["Lat stretch (hang from bar)", "30s each side"],
          ["Seated hamstring stretch", "45s each leg"],
          ["Foam roll lower back & lats", "2 min"],
          ["Child's pose", "1 min"]
        ]
      }
    },
    "cooldown-done": {
      "type": "Checkbox",
      "props": {
        "label": "Cooldown completed",
        "checked": { "$bindState": "/completed/cooldown" }
      }
    },
    "session-progress-card": {
      "type": "Card",
      "props": {},
      "children": ["session-progress-stack"]
    },
    "session-progress-stack": {
      "type": "Stack",
      "props": { "gap": "md" },
      "children": ["session-progress-title", "session-stats", "session-progress-bar"]
    },
    "session-progress-title": {
      "type": "Heading",
      "props": { "level": "h3", "text": "Session Summary" }
    },
    "session-stats": {
      "type": "Table",
      "props": {
        "columns": ["Metric", "Value"],
        "rows": [
          ["Total working sets", "26"],
          ["Estimated duration", "~55 min"],
          ["Primary focus", "Deadlift strength"],
          ["Split day", "Pull"]
        ]
      }
    },
    "session-progress-bar": {
      "type": "Progress",
      "props": { "value": 0 }
    }
  },
  "state": {
    "/completed/warmup": false,
    "/completed/mainLifts": false,
    "/completed/accessories": false,
    "/completed/cooldown": false
  }
}
